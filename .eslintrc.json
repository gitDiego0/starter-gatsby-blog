{
  "env": {
    "es2021": true,
    "browser": true,
    "node": true
  },
  "extends": ["standard", "prettier", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 0,
    "non-unused-vars": "warn"
  }
}
